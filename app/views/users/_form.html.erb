<%= form_for(user) do |f| %>
  <%= f.label :username %><br>
  <%= f.text_field :username %><br>
  <br>
  <br>
  <%= f.label :email_address %><br>
  <%= f.text_field :email_address %><br>
  <br>
  <br>
  <%= f.label :password_digest %><br>
  <%= f.text_field :password_digest %><br>
  <br>
  <br>
  <%= f.submit %>
<% end %>
